						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>
				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink text="บัญชีภาษีเบื้องต้น" icon="mdi-action-shopping-cart"
								textColor="brown" />
						</m:header>
						<m:content>
							<m:MaterialLabel
								text="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat." />
						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>
				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink text="ภาษีที่ดิน" icon="mdi-image-style"
								textColor="blue" />
						</m:header>
						<m:content>
							<m:MaterialLabel
								text="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat." />
						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>
				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink text="ภาษีบุคคลธรรมดา" icon="mdi-editor-insert-chart"
								textColor="green" />
						</m:header>
						<m:content>
							<m:MaterialLabel
								text="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat." />
						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>
			</m:MaterialCollapsible>
		</m:MaterialContainer>

		<m:MaterialFloatingContainer>
			<m:initial>
				<m:MaterialButton type="floating" waves="light"
					size="large" color="red" icon="mdi-content-add" tooltip="Create"
					tooltipLocation="left" />
			</m:initial>
			<m:item>
				<m:MaterialButton type="floating" waves="light"
					color="yellow" icon="mdi-communication-email" tooltip="Email"
					tooltipLocation="left" />
			</m:item>
			<m:item>
				<m:MaterialButton type="floating" waves="light"
					color="blue" icon="mdi-image-palette" tooltip="Theme"
					tooltipLocation="left" />
			</m:item>
		</m:MaterialFloatingContainer>

		<m:MaterialFooter color="blue"
			copyright="© 2015 Copyright GWT Material" type="fixed">

		</m:MaterialFooter>
	</g:HTMLPanel>
</ui:UiBinder> 
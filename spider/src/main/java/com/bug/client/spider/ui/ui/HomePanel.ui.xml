<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.label {
		margin-left: 5%;
		margin-top: 5%
	}
	
	header, main, footer {
		padding-left: 300px;
	}
	</ui:style>
	<g:HTMLPanel>
		<m:MaterialNavBar wave="light" align="right" color="blue"
			text="Starter" sideBar="fixed" sideBarWidth="300">
			<m:nav>
				<m:MaterialSearch />
			</m:nav>
			<m:nav>
				<m:MaterialLink icon="mdi-social-whatshot" textColor="white"
					tooltip="Starter" />
			</m:nav>
			<m:nav>
				<m:MaterialLink icon="mdi-navigation-apps" textColor="white"
					tooltip="Apps" />
			</m:nav>
			<m:nav>
				<m:MaterialLink icon="mdi-social-notifications"
					textColor="white" tooltip="Notification" />
			</m:nav>
			<m:sidebaritem>
				<m:MaterialTopNav profileName="GWT Material"
					profileImageUrl="http://b.vimeocdn.com/ps/339/488/3394886_300.jpg"
					url="http://2.bp.blogspot.com/-x4D3EPELYRU/VMHu52a4qUI/AAAAAAAAHek/EmnIgqTtu_U/s0/fon116%2B2160x1920.png"
					ui:field="topNav" textColor="white" type="sidebar">
					<m:child>
						<m:MaterialLink text="gwt-material@gmail.com"
							textColor="white" fontSize="0.8" icon="mdi-navigation-arrow-drop-down"
							iconPosition="right" />
					</m:child>
				</m:MaterialTopNav>
			</m:sidebaritem>
			<m:sidebaritem>
				<m:MaterialLink icon="mdi-social-whatshot"
					iconPosition="left" text="Starter" textColor="blue" />
			</m:sidebaritem>
			<m:sidebaritem>
				<m:MaterialLink icon="mdi-navigation-apps"
					iconPosition="left" text="Apps" textColor="blue" />
			</m:sidebaritem>
			<m:sidebaritem>
				<m:MaterialLink icon="mdi-social-notifications"
					iconPosition="left" text="Notifications" textColor="blue"
					separator="true" />
			</m:sidebaritem>
			<m:sidebaritem>
				<m:MaterialLink icon="mdi-action-explore"
					iconPosition="left" text="Navigation" textColor="blue" />
			</m:sidebaritem>
			<m:sidebaritem>
				<m:MaterialLink icon="mdi-action-grade" iconPosition="left"
					text="Favorites" textColor="blue" />
			</m:sidebaritem>
		</m:MaterialNavBar>

		<m:MaterialContainer>
			<m:MaterialLink text="รายการเปิดอบรม" textColor="grey"
				addStyleNames="{style.label}" />
			<m:MaterialCollapsible type="popout"  ui:field="popout" >
				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink text="Accounting 101" icon="mdi-maps-flight"
								textColor="purple" />
						</m:header>
						<m:content>
						
						<m:MaterialRow>
									 	<m:MaterialLabel
									text="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat." />
							
									<m:MaterialButton text="ลงทะเบียน" ></m:MaterialButton>
						</m:MaterialRow>
						
						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>
				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink addStyleNames="active" text="e-Filling"
								icon="mdi-social-people" textColor="red" />
						</m:header>
						<m:content>
							<m:MaterialPanel>
								<m:MaterialLabel
									text="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat." />
								<m:MaterialRow>
									<m:MaterialTextBox placeholder="CitizenID" />
									<m:MaterialTextBox placeholder="AcountingID" />
									<m:MaterialTextBox placeholder="First Name" />
									<m:MaterialTextBox placeholder="Last Name" />
									<m:MaterialTextBox placeholder="Email" />
									<m:MaterialTextBox placeholder="Telephone" />
									<m:MaterialButton text="Save" ></m:MaterialButton>

								</m:MaterialRow>
							</m:MaterialPanel>
						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>
				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink text="ภาษีมรดก" icon="mdi-content-flag"
								textColor="orange" />
						</m:header>
						<m:content>
							<m:MaterialLabel
